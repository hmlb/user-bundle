<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                   https://raw.github.com/doctrine/doctrine2/master/doctrine-mapping.xsd">
    <embeddable name="HMLB\UserBundle\Message\Trace\Context">
        <field name="phpInterface" type="string" length="100" nullable="false"/>
        <field name="cli" type="boolean"  nullable="false"/>
        <field name="arguments" type="text" nullable="true"/>
        <field name="requestMethod" type="string" nullable="true" length="8"/>
        <field name="path" type="string" nullable="true"/>
        <field name="host" type="string" nullable="true"/>
    </embeddable>
</doctrine-mapping>
