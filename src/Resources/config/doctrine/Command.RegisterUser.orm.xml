<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                   https://raw.github.com/doctrine/doctrine2/master/doctrine-mapping.xsd">
    <entity name="HMLB\UserBundle\Command\RegisterUser" table="hmlb_user_command_registeruser">
        <field name="userIdentity" type="ddd_identity" nullable="false"/>
        <field name="username" type="string" length="100" nullable="false"/>
        <field name="email" type="string" nullable="false"/>
        <field name="roles" type="array" nullable="false"/>
    </entity>
</doctrine-mapping>
